# Knik Console Configuration Template
# Copy this file to .env and customize your settings

# Log Settings
export KNIK_SHOW_LOGS=True
export KNIK_LOG_LEVEL=INFO
export KNIK_USE_COLORS=True

# Python Settings
# Force native ARM64 execution (no Rosetta)
export ARCHFLAGS="-arch arm64"

# Console Settings
export KNIK_VOICE_OUTPUT=True
export KNIK_VOICE_NAME=af_sarah

# AI Settings
export KNIK_AI_PROVIDER=vertex
export KNIK_AI_MODEL=gemini-2.5-flash
export KNIK_AI_PROJECT_ID=your-project-id-here
export KNIK_AI_LOCATION=asia-south1
export KNIK_AI_SYSTEM_INSTRUCTION="You are a helpful AI assistant with voice capabilities. Be concise and conversational."
